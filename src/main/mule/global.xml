	 <api-gateway:autodiscovery apiId="${secure::Autodiscovery.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="aa476a69-4d42-4ffd-a5df-e70f515e0c15" flowRef="mi-corp-salesforce-sys-api-main" />
